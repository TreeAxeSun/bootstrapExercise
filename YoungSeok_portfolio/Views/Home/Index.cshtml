                                        <div class="col-md-12">
                                            <button id="btnErase" class="btn btn-danger">Clear</button>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-md-12">
                                            <button id="btnCodefizzbuzz" class="btn btn-success">Show/Hide code</button>
                                        </div>
                                    </div>
                                    <br /><br /><hr />
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <input id="fizzNum" type="number" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <input id="buzzNum" type="number" />
                                        </div>
                                    </div>
                                </div>
